{
  "manifest_version": 2,
  "name": "Cheeky Avo",
  "description": "You can't stop the signal.",
  "version": "1.0.4",
  "permissions": [],
  "icons": {
    "16": "icons/avo16.png",
    "32": "icons/avo32.png",
    "48": "icons/avo48.png",
    "128": "icons/avo128.png"
  },
  "content_scripts": [
    {
		"matches": [
			"https://*.trademe.co.nz/*", "https://*.realestate.co.nz/*"
		],
		"js": ["jquery.js", "app.js"],
		"run_at": "document_end"
    }
  ]
}